    Or(a=b, b=false, out=out[4]);
    Or(a=c, b=false, out=out[3]);
    Or(a=d, b=false, out=out[2]);
    Or(a=e, b=false, out=out[1]);
    Or(a=f, b=false, out=out[0]);
}
